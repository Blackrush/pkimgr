%p#notice= notice
%h1 Cert Profiles
%table
  %thead
    %tr
      %th Name
      %th{:colspan => "3"}
  %tbody
    - @cert_profiles.each do |cert_profile|
      %tr
        %td= cert_profile.name
        %td= link_to 'Show', cert_profile
        %td= link_to 'Edit', edit_cert_profile_path(cert_profile)
        %td= link_to 'Destroy', cert_profile, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Cert Profile', new_cert_profile_path
